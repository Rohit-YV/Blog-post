<%- include("partials/header") -%>
<h1>HOME</h1>
<p><%= startingContent %></p>
<% posts.forEach((post) => { %>
    <!-- Display each post title, you can add more content here as needed -->
    <h2><%= post.title %></h2>
    <p><%= post.content.substring(0,100) + "...." %>
    <a href="/posts/<%= post.title %>">Read MORE</a>
    </p>
<% }); %>

<%- include("partials/footer") -%>
